# Dispatch Environment Variables
# Copy to .env.local and fill in your values

# Janua SSO (Authentication)
NEXT_PUBLIC_JANUA_URL=https://api.janua.dev

# Cloudflare API (Server-side only)
# Get your API token from: https://dash.cloudflare.com/profile/api-tokens
# Required permissions: Zone:Read, Zone:Edit, DNS:Read, DNS:Edit
CLOUDFLARE_API_TOKEN=your_cloudflare_api_token_here
CLOUDFLARE_ACCOUNT_ID=your_cloudflare_account_id_here

# App URL (for OAuth redirect)
NEXT_PUBLIC_APP_URL=http://localhost:4203
